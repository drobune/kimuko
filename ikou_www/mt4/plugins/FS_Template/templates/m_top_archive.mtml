<MTEntries tags="トップ表示記事">
<div id="top-entry">
	<h3 class="title"><$MTEntryTitle$></h3>
	<div class="content">
	<$MTEntryBody$>
	<div class="clear"></div>
	</div>
	<MTIfNonEmpty tag="EntryMore" convert_breaks="0">
	<div class="asset-more-link">
	<a href="<$MTEntryPermalink$>">詳細はこちら</a>
	</div>
	</MTIfNonEmpty>
</div>
</MTEntries>
